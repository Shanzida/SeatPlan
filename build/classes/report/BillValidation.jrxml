	<queryString>
		<![CDATA[SELECT distinct
		 s.room_no
		,case when  s.id_c1 is not null then s.id_c1 else n.id_c1 end as id_c1
		,case when s. row_c1 is not null then  s. row_c1 else  n. row_c1 end as   rowc1
		,case when s. course_c1  is not null then  s. course_c1 else  n. course_c1 end as   course_c1
		,case when s. sec_c1 is not null then  s. sec_c1 else  n. sec_c1 end as   sec_c1

		,case when  s.id_c2 is not null then s.id_c2 else n.id_c2 end as id_c2
		,case when s. row_c2 is not null then  s. row_c2 else  n. row_c2 end as   rowc2
		,case when s. course_c2  is not null then  s. course_c2 else  n. course_c2 end as   course_c2
		,case when s. sec_c2  is not null then  s. sec_c2 else  n. sec_c2 end as   sec_c2

		,case when  s.id_c3 is not null then s.id_c3 else n.id_c3 end as id_c3
		,case when s. row_c3 is not null then  s. row_c3 else  n. row_c3 end as   rowc3
		,case when s. course_c3  is not null then  s. course_c3 else  n. course_c3 end as   course_c3
		,case when s. sec_c3 is not null then  s. sec_c3 else  n. sec_c3 end as   sec_c3

		,case when  s.id_c4 is not null then s.id_c4 else n.id_c4 end as id_c4
		,case when s. row_c4 is not null then  s. row_c4 else  n. row_c4 end as   rowc4
		,case when s. course_c4  is not null then  s. course_c4 else  n. course_c4 end as   course_c4
		,case when s. sec_c4 is not null then  s. sec_c4 else  n. sec_c4 end as   sec_c4

From

(
SELECT 	case when mn. room_no is not null then mn. room_no
			when m. room_no is not null then m. room_no
			when a. room_no is not null then a. room_no
			when b. room_no is not null then b. room_no
			when c. room_no is not null then c. room_no
		end as  room_no
		,m.id as id_c1, m.row_no as row_c1,m.course_code as course_c1,m.section as sec_c1
		,a.id as id_c2, a.row_no as row_c2,a.course_code as course_c2,a.section as sec_c2
    		,b.id as id_c3, b.row_no as row_c3,b.course_code as course_c3,b.section as sec_c3
      		 ,c.id as id_c4, c.row_no as row_c4,c.course_code as course_c4,c.section as sec_c4

FROM  (Select distinct room_no,row_no from `arrangement` where date=$P{dd1} and time=$P{time}) mn
left  join
( Select id, row_no,course_code ,section, room_no from `arrangement`  m  where  m.column_no=1 ) m on (m.room_no=mn.room_no and mn.row_no=m.row_no )
left Join
( Select  id, row_no,course_code ,section, room_no  from `arrangement`  a  where  a.column_no=2 ) a on  (a.room_no=mn.room_no and mn.row_no=a.row_no )
left Join
( Select   id,row_no,course_code ,section, room_no  from `arrangement`  b  where  b.column_no=3 ) b on  (b.room_no=mn.room_no and mn.row_no=b.row_no )
left Join
( Select    id,row_no,course_code ,section, room_no from `arrangement`  c  where  c.column_no=4 ) c  on  (c.room_no=mn.room_no and mn.row_no=c.row_no )
)s

left join

(SELECT 	case when mn. room_no is not null then mn. room_no
			when m. room_no is not null then m. room_no
			when a. room_no is not null then a. room_no
			when b. room_no is not null then b. room_no
			when c. room_no is not null then c. room_no
		end as  room_no
		,m.id as id_c1, m.row_no as row_c1,m.course_code as course_c1,m.section as sec_c1
		,a.id as id_c2, a.row_no as row_c2,a.course_code as course_c2,a.section as sec_c2
    		,b.id as id_c3, b.row_no as row_c3,b.course_code as course_c3,b.section as sec_c3
      		 ,c.id as id_c4, c.row_no as row_c4,c.course_code as course_c4,c.section as sec_c4

FROM  (Select distinct room_no,row_no from `arrangement` where date=$P{dd1} and time=$P{time}) mn
right join
( Select id, row_no,course_code ,section, room_no from `arrangement`  m  where  m.column_no=1 ) m on (m.room_no=mn.room_no and mn.row_no=m.row_no )
right Join
( Select  id, row_no,course_code ,section, room_no  from `arrangement`  a  where  a.column_no=2 ) a on  (a.room_no=mn.room_no and mn.row_no=a.row_no )
right Join
( Select   id,row_no,course_code ,section, room_no  from `arrangement`  b  where  b.column_no=3 ) b on  (b.room_no=mn.room_no and mn.row_no=b.row_no )
right Join
( Select    id,row_no,course_code ,section, room_no from `arrangement`  c  where  c.column_no=4 ) c  on  (c.room_no=mn.room_no and mn.row_no=c.row_no )
) n on(s.room_no=n.room_no)
order by room_no,(case when rowc1 is null then 100 else rowc1 end ),(case when rowc2 is null then 100 else rowc2 end ),(case when rowc3 is null then 100 else rowc3 end ),(case when rowc4 is null then 100 else rowc4 end )]]>
	</queryString>
